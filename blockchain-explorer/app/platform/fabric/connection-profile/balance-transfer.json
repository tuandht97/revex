{
	"name": "trit",
	"version": "1.0.0",
	"license": "Apache-2.0",
	"client": {
		"tlsEnable": true,
		"adminUser": "admin",
		"adminPassword": "adminpw",
		"enableAuthentication": true,
		"organization": "TraderOrgMSP",
		"connection": {
			"timeout": {
				"peer": {
					"endorser": "300"
				},
				"orderer": "300"
			}
		}
	},
	"channels": {
		"tritchannel": {
			"orderers": ["orderer0"],
			"peers": {
				"trader-peer": {
					"ledgerQuery": true
				}
			}
		}
	},
	"organizations": {
		"TraderOrgMSP": {
			"mspid": "TraderOrgMSP",
			"peers": ["trader-peer"],
			"certificateAuthorities": ["trader-ca"],
			"adminPrivateKey": {
				"path": "crypto/peerOrganizations/trader-org/users/Admin@trader-org/msp/keystore/941599f16aea7b3477f35650cbb9703d599d86675fdab7c1ffac58317755629b_sk"
			},
			"signedCert": {
				"path": "crypto/peerOrganizations/trader-org/users/Admin@trader-org/msp/signcerts/Admin@trader-org-cert.pem"
			}
		}
	},

	"peers": {
		"trader-peer": {
			"url": "grpcs://localhost:9051",
			"grpcOptions": {
				"ssl-target-name-override": "trader-peer"
			},
			"tlsCACerts": {
				"path": "crypto/peerOrganizations/trader-org/tlsca/tlsca.trader-org-cert.pem"
			}
		}
	},
	"certificateAuthorities": {
		"trader-ca": {
			"url": "https://localhost:9054",
			"httpOptions": {
				"verify": false
			},
			"tlsCACerts": {
				"path": "crypto/peerOrganizations/trader-org/ca/ca.trader-org-cert.pem"
			},

			"caName": "trader-org"
		}
	}
}
