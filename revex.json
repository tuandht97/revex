{
	"name": "revex",
	"version": "1.0.0",
	"license": "Apache-2.0",
	"client": {
		"tlsEnable": true,
		"adminUser": "admin",
		"adminPassword": "adminpw",
		"enableAuthentication": true,
		"organization": "CcqOrgMSP",
		"connection": {
			"timeout": {
				"peer": {
					"endorser": "300"
				},
				"orderer": "300"
			}
		}
	},
	"channels": {
		"revex": {
			"orderers": ["orderer0"],
			"peers": {
				"ccq-peer": {
					"ledgerQuery": true
				}
			}
		}
	},
	"organizations": {
		"CcqOrgMSP": {
			"mspid": "CcqOrgMSP",
			"peers": ["ccq-peer"],
			"certificateAuthorities": ["ccq-ca"],
			"adminPrivateKey": {
				"path": "crypto/peerOrganizations/ccq-org/users/Admin@ccq-org/msp/keystore/0a65199e6463db20f38230855dbfb585c10c69de24cef72564d75fc98a1ea537_sk"
			},
			"signedCert": {
				"path": "crypto/peerOrganizations/ccq-org/users/Admin@ccq-org/msp/signcerts/Admin@ccq-org-cert.pem"
			}
		}
	},

	"peers": {
		"ccq-peer": {
			"url": "grpcs://localhost:40051",
			"grpcOptions": {
				"ssl-target-name-override": "ccq-peer"
			},
			"tlsCACerts": {
				"path": "crypto/peerOrganizations/ccq-org/tlsca/tlsca.ccq-org-cert.pem"
			}
		}
	},
	"certificateAuthorities": {
		"ccq-ca": {
			"url": "https://localhost:40054",
			"httpOptions": {
				"verify": false
			},
			"tlsCACerts": {
				"path": "crypto/peerOrganizations/ccq-org/ca/ca.ccq-org-cert.pem"
			},

			"caName": "ccq-org"
		}
	}
}